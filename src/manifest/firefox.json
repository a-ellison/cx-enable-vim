{
  "manifest_version": 2,
  "name": "CX Enable Vim",
  "version": "1.5",
  "description": "Enables vim mode in the javascript IDE (https://ace.c9.io/) used in Code Expert (https://expert.ethz.ch/).",
  "content_scripts": [
    {
      "matches": ["*://expert.ethz.ch/*"],
      "js": ["js/ace-enable-vim.js"]
    }
  ],
  "permissions": ["storage"],
  "background": {
    "scripts": ["js/toggle.js"]
  },
  "web_accessible_resources": [
    "js/ace-keybinding-vim.js",
    "js/set-vim-keyboard-handler.js",
    "js/set-default-keyboard-handler.js"
  ],
  "browser_action": {
    "default_icon": "icons/icon.png"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "3486b8b3-982a-482c-8dfb-77ad9c4550c3@example.com"
    }
  }
}
